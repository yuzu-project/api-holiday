{
    "env": {
        "DefaultLibraryPath": "${workspaceFolder}/libs",
        "CppRestSdkBaseFolder": "${DefaultLibraryPath}/cpprestsdk",
        "DefaultIncludePath": [
            "${workspaceFolder}/src/include",
            "${CppRestSdkBaseFolder}/Release/include"
        ],
        "CommonDefines": []
    },
    "configurations": [
        {
            "name": "Debug/Linux|x64",
            "includePath": [
                "${DefaultIncludePath}"
            ],
            "defines": [
                "${CommonDefines}",
                "DEBUG",
                "_DEBUG"
            ],
            "compileCommands": "${default}",
            "macFrameworkPath": [
                "${default}"
            ],
            "forcedIncludes": [
                "${default}"
            ],
            "intelliSenseMode": "${default}",
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "limitSymbolsToIncludedHeaders": false,
                "databaseFilename": "${default}",
                "path": [
                    "${default}"
                ]
            }
        }
    ],
    "version": 4
}